<Window x:Class="AppHost.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppHost"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1050">
    <Grid>
        <ContentControl  x:Uid="ContentControl_1" d:DesignHeight="350" d:DesignWidth="525"  Content="{Binding Path=CurrentScreen}" Margin="0,0,0,0" />
    </Grid>
</Window>
